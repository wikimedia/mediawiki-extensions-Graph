{
	"name": "Graph",
	"version": "0.0.0",
	"private": true,
	"description": "Build tools for the Graph-MediaWiki extension.",
	"scripts": {
		"test:unit": "jest tests/jest --silent",
		"fixup": "echo \"/*@nomin*/\"|cat - lib/vega5/vega.js > /tmp/out && mv /tmp/out lib/vega5/vega.js",
		"build-lib": "cp node_modules/vega/build-es5/vega.js lib/vega5/vega.js && npm run fixup",
		"test": "grunt test && npm run test:unit"
	},
	"devDependencies": {
		"browserify": "^16.2.3",
		"eslint-config-wikimedia": "0.24.0",
		"grunt": "1.6.1",
		"grunt-banana-checker": "0.10.0",
		"grunt-eslint": "24.0.0",
		"grunt-stylelint": "0.18.0",
		"jest": "27.4.7",
		"jquery.splitter": "^0.27.1",
		"mw-graph-shared": "^0.6.0",
		"stylelint-config-wikimedia": "0.14.0",
		"vega": "5.24.0"
	}
}
